{
    "env": {
        "browser": true,
        "jquery": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": ["warn", 2],
        "max-len": ["warn", { "code": 100 }],
        "quotes": ["warn", "single"],
        "semi": ["warn", "always"]
    }
}
